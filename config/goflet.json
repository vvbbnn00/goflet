{
  "httpConfig": {
    "host": "0.0.0.0",
    "port": 8080,
    "httpsConfig": {
      "enabled": false,
      "cert": "",
      "key": ""
    }
  },
  "fileConfig": {
    "baseFileStoragePath": "data",
    "allowFolderCreation": true,
