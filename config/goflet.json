  "jwtConfig": {
    "algorithm": "HS256",
    "Security": {
      "signingKey": "goflet",
      "publicKey": "",
      "privateKey": ""
    },
    "trustedIssuers": null
  },
  "cronConfig": {
    "deleteEmptyFolder": 3600,
    "cleanOutdatedFile": 3600
  }
}
